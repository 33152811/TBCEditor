                "CloseToken": "}"
            }
        ]
    },
    "CompletionProposal": {
        "SkipRegion": [
            {
                "OpenToken": "'",
                "CloseToken": "'",
                "RegionType": "String"
            },
            {
                "OpenToken": "\"",
                "CloseToken": "\"",
                "RegionType": "String"
            },
            {
                "OpenToken": "#",
                "RegionType": "SingleLine"
            }
        ]
    }
