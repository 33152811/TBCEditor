{
    "Highlighter": {
        "Info": {
            "General": {
                "Version": "1.0",
                "Date": "1.5.2015",
                "Sample": "# Hello World in AWK\r\nBEGIN { print \"Hello, world!\" }"
            },
            "Author": {
                "Name": "Lasse Rautiainen",
                "Mail": "lasse@bonecode.com"
            }
        },
        "MainRules": {
            "Attributes": {
                "Element": "Editor"
            },
            "SubRules": {
                "Range": [
                    {
                        "Type": "Comment",
                        "Attributes": {
                            "Element": "Comment",
                            "ParentBackground": true
                        },
                        "Properties": {
                            "CloseOnEol": true
                        },
                        "TokenRange": {
                            "Open": "#"
                        }
                    },
                    {
                        "Type": "Character",
                        "Attributes": {
                            "Element": "Character",
                            "ParentBackground": true
                        },
                        "Properties": {
                            "CloseOnTerm": true,
                            "CloseOnEol": true
                        },
                        "TokenRange": {
                            "Open": "$",
                            "Close": " "
                        }
                    },
                    {
                        "Type": "String",
                        "Attributes": {
                            "Element": "String",
                            "ParentBackground": true
                        },
                        "Properties": {
                            "CloseOnEol": true
                        },
                        "TokenRange": {
                            "Open": "'",
                            "Close": "'"
                        },
                        "SubRules": {
                            "Range": [
                                {
                                    "Type": "String",
                                    "Attributes": {
                                        "Element": "String",
                                        "ParentBackground": true
                                    },
                                    "Properties": {
                                        "CloseOnEol": true,
                                        "CloseOnTerm": true
                                    },
                                    "TokenRange": {
                                        "Open": "\\'",
                                        "Close": ""
                                    }
                                },
                                {
                                    "Type": "String",
                                    "Attributes": {
                                        "Element": "String",
                                        "ParentBackground": true
                                    },
                                    "Properties": {
                                        "CloseOnEol": true,
                                        "CloseOnTerm": true
                                    },
                                    "TokenRange": {
                                        "Open": "\\\\",
                                        "Close": ""
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "Type": "String",
                        "Attributes": {
                            "Element": "String",
                            "ParentBackground": true
                        },
                        "Properties": {
                            "CloseOnEol": true
                        },
                        "TokenRange": {
                            "Open": "\"",
                            "Close": "\""
                        },
                        "SubRules": {
                            "Range": [
                                {
                                    "Type": "String",
                                    "Attributes": {
                                        "Element": "String",
                                        "ParentBackground": true
                                    },
                                    "Properties": {
                                        "CloseOnEol": true,
                                        "CloseOnTerm": true
                                    },
                                    "TokenRange": {
                                        "Open": "\\\"",
                                        "Close": ""
                                    }
                                },
                                {
                                    "Type": "String",
                                    "Attributes": {
                                        "Element": "String",
                                        "ParentBackground": true
                                    },
                                    "Properties": {
                                        "CloseOnEol": true,
                                        "CloseOnTerm": true
                                    },
                                    "TokenRange": {
                                        "Open": "\\\\",
                                        "Close": ""
                                    }
                                }
                            ]
                        }
                    }
                ],
                "KeyList": [
                    {
                        "Type": "ReservedWord",
                        "Words": "BEGIN\r\nbreak\r\ncontinue\r\ndo\r\nelse\r\nEND\r\nexit\r\nfor\r\nfunction\r\ngetline\r\nif\r\nnext\r\nprint\r\nreturn\r\nwhile",
                        "Attributes": {
                            "Element": "ReservedWord",
                            "ParentBackground": true
                        }
                    },
                    {
                        "Type": "ReservedWord",
                        "Words": "FS\r\nNF\r\nRS\r\nNR\r\nFILENAME\r\nOFS\r\nORS\r\nARGC\r\nARGV\r\nARGIND\r\nFNR\r\nOFMT\r\nRSTART\r\nRLENGTH\r\nSUBSEP\r\nENVIRON\r\nIGNORECASE\r\nCONVFMT\r\nERRNO\r\nFIELDWIDTHS\r\nBINMODE\r\nLINT\r\nPROCINFO\r\nRT\r\nTEXTDOMAIN",
                        "Attributes": {
                            "Element": "Directive",
                            "ParentBackground": true
                        }
                    },
                    {
                        "Type": "Symbol",
                        "Words": "..\r\n:\r\n;\r\n<\r\n>\r\n=\r\n/\r\n*\r\n.\r\n,\r\n-\r\n+\r\n(\r\n)\r\n[\r\n]\r\n{\r\n}",
                        "Attributes": {
                            "Element": "Symbol",
                            "ParentBackground": true
                        }
                    }
                ],
                "Set": [
                    {
                        "Type": "Numbers",
                        "Symbols": "0123456789",
                        "Attributes": {
                            "Element": "Number",
                            "ParentBackground": true
                        }
                    },
                    {
                        "Type": "Float",
                        "Symbols": ".0123456789",
                        "Attributes": {
                            "Element": "Number",
                            "ParentBackground": true
                        }
                    }
                ]
            }
        }
    },
