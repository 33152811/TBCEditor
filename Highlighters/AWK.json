                "RegionType": "SingleLine"
            }
        ],
        "FoldRegion": [
            {
                "OpenToken": "{",
                "CloseToken": "}"
            }
        ]
    },
    "MatchingPair": {
        "Pairs": [
            {
                "OpenToken": "(",
                "CloseToken": ")"
            },
            {
                "OpenToken": "[",
                "CloseToken": "]"
            },
            {
